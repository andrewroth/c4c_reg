<h1>Editing field_value</h1>

<% form_for(@field_value) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :field_id %><br />
    <%= f.text_field :field_id %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_area :value %>
  </p>
  <p>
    <%= f.label :registration_id %><br />
    <%= f.text_field :registration_id %>
  </p>
  <p>
    <%= f.label :updated_at %><br />
    <%= f.datetime_select :updated_at %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @field_value %> |
<%= link_to 'Back', field_values_path %>